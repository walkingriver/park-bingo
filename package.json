{
  "name": "park-pursuit-bingo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "fetch-images": "node scripts/fetch-images.js",
    "deploy": "ng build && wrangler pages deploy dist/park-bingo/browser --project-name=park-bingo --branch=main",
    "android:sync": "ng build && npx cap sync android",
    "android:open": "npx cap open android",
    "android:build": "ng build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:bundle": "ng build && npx cap sync android && cd android && ./gradlew bundleRelease",
    "ios:sync": "ng build && npx cap sync ios",
    "ios:open": "npx cap open ios",
    "store:assets": "node scripts/generate-store-assets.mjs",
    "store:resize": "cd store-assets && sips -z 1920 1080 screenshot-*.png && sips -z 500 1024 feature-graphic.png && sips -z 512 512 play-store-icon.png"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.4",
  "dependencies": {
    "@angular/animations": "^21.0.0",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/service-worker": "^21.0.0",
    "@capacitor/android": "^7.5.0",
    "@capacitor/angular": "^2.0.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.3",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.5.0",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/preferences": "^7.0.3",
    "@capacitor/share": "^7.0.3",
    "@capacitor/splash-screen": "^7.0.4",
    "@capacitor/status-bar": "^7.0.3",
    "@ionic/angular": "^8.7.10",
    "@types/canvas-confetti": "^1.9.0",
    "@types/uuid": "^10.0.0",
    "canvas-confetti": "^1.9.4",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "fs-extra": "^11.3.2",
    "rxjs": "~7.8.0",
    "sharp": "^0.34.5",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "latest",
    "@playwright/test": "^1.58.2",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.18",
    "wrangler": "^4.65.0"
  }
}
