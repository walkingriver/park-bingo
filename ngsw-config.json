{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.csr.html",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    },
    {
      "name": "data",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/data/**/*.json"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "remote-parks",
      "urls": [
        "https://raw.githubusercontent.com/walkingriver/park-bingo/main/public/data/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "affiliate-products",
      "urls": [
        "/data/affiliate-products.json"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "7d",
        "timeout": "3s"
      }
    },
    {
      "name": "amazon-images",
      "urls": [
        "https://ws-na.amazon-adsystem.com/**",
        "https://m.media-amazon.com/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "30d"
      }
    }
  ]
}
